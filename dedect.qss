QToolTip {
    border: 2px solid darkkhaki;
    background-color: #33375B;
    color: white;
    padding: 0px;
    border-radius: 3px;
    opacity: 500;
}

QMdiSubWindow {
    color: black;
    height: 25px;
    font: bold 24px;
    border: 2px solid #000000;
    background-color: black;
}
QMdiSubWindow::title {
    background-color: black;
    border-width: 0px;
    margin: 0px 0px 0px 0px;
    padding: 2px 20px 2px 20px;
    border: 0px solid transparent;
}
QMdiSubWindow::close-button, QMdiSubWindow::float-button {
    background: darkgray;
}
QMdiSubWindow::close-button:hover, QMdiSubWindow::float-button:hover {
    background: gray;
}
QMdiSubWindow::close-button:pressed, QMdiSubWindow::float-button:pressed {
}
QMdiSubWindow::separator {
    background: darkgray;
    width: 10px;
    height: 10px;
}

