MQDockWidgetFilterOptions QTreeView {
    show-decoration-selected: 1;
}
MQDockWidgetFilterOptions QTreeView::item {
    border: 1px solid #33375B;
    border-top-color: transparent;
    border-bottom-color: transparent;
}
MQDockWidgetFilterOptions QTreeView::item:hover {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #e7effd, stop: 1 #33375B);
    border: 1px solid #bfcde4;
}
MQDockWidgetFilterOptions QTreeView::item:selected {
    border: 1px solid #33375B;
}
MQDockWidgetFilterOptions QTreeView::item:selected:active{
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #6ea1f1, stop: 1 #33375B);
}
MQDockWidgetFilterOptions QTreeView::item:selected:!active {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #6b9be8, stop: 1 #33375B);
}
MQDockWidgetFilterOptions QHeaderView::section {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #6ea1f1, stop: 1 #33375B);
    color: white;
    padding-left: 30px;
    border: 1px solid #33375B;
    border-top-color: transparent;
    border-bottom-color: transparent;
}

